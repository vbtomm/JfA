package mooc.vandy.java4android.calculator.logic;

/**
 * Perform the Multiply operation.
 */
public class Multiply {

    // TODO - add your solution here.

    //initial setter
    private int mulArgumentOne = 0;
    private int mulArgumentTwo = 0;


    //getter/setter
    public Multiply(int argumentOne, int argumentTwo) {
        mulArgumentOne = argumentOne;
        mulArgumentTwo = argumentTwo;
    }

    //Here we can get the string result of operation
    public String toString() {
        return String.valueOf(mulArgumentOne * mulArgumentTwo);
    }
}
